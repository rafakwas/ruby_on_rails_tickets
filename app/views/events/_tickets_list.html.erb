<p id="notice"><%= notice %></p>

<h1>Bilety</h1>

<table>
  <thead>
    <tr>
      <th>Imię</th>
      <th>Siedzenie</th>
      <th>Adres</th>
      <th>Cena</th>
      <th>Email</th>
      <th>Telefon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.seat_id_seq %></td>
        <td><%= ticket.address %></td>
        <td><%= ticket.price %></td>
        <td><%= ticket.email_address %></td>
        <td><%= ticket.phone %></td>
        <td><%= link_to 'Pokaż', ticket %></td>
        <td><%= link_to 'Edytuj', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Usuń', ticket, method: :delete, data: { confirm: 'Jesteś pewien?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
